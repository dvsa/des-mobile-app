{
  "name": "des-mobile-app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "test": "ng test --no-watch",
    "test:watch": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "remote-devtools-server": "./bin/remote-devtools-server.sh",
    "serve:local": "npm run config:local && ionic serve",
    "serve:emulator": "npm run config:dev-tools && ionic cap run ios",
    "serve:ipad:notworking": "npm run config:dev && npm run schema-version",
    "open:ios": "rm -rf ios && npm run build && ionic cap add ios & ionic cap copy ios && ionic cap open ios",
    "build": "npm run copy-resources && ionic build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "stylelint": "stylelint 'src/**/*.scss' fix --config stylelint.json",
    "gitSecrets": "git secrets --scan",
    "scanRepo": "git log -p -n 15 | scanrepo",
    "e2e": "ng e2e",
    "config:dev-tools": "cp src/environments/environment.dev.dev-tools.ts src/environments/environment.ts",
    "config:dev": "cp src/environments/environment.dev.ts src/environments/environment.ts",
    "config:local": "cp src/environments/environment.local.ts src/environments/environment.ts",
    "schema-version": "buildScripts/generate-test-schema-version.js",
    "copy-resources": "rm -r ios/App/App/Assets.xcassets/AppIcon.appiconset* && cp -r resources/ios/icon/ ios/App/App/Assets.xcassets/AppIcon.appiconset && rm -r ios/App/App/Assets.xcassets/Splash.imageset/* && cp -r resources/ios/splash/ ios/App/App/Assets.xcassets/Splash.imageset"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@capacitor/core": "2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@dvsa/mes-config-schema": "1.1.0",
    "@dvsa/mes-journal-schema": "1.2.0",
    "@ionic-enterprise/auth": "^3.1.4",
    "@ionic-native-mocks/secure-storage": "^2.0.12",
    "@ionic-native/app-version": "^5.30.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/device": "^5.30.0",
    "@ionic-native/network": "^5.29.0",
    "@ionic-native/secure-storage": "^5.29.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "@ngrx/effects": "^10.1.0",
    "@ngrx/entity": "^10.1.0",
    "@ngrx/store": "^10.1.0",
    "@ngrx/store-devtools": "^10.1.0",
    "cordova-ios": "5.1.1",
    "cordova-plugin-app-version": "^0.1.12",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-plugin-secure-storage-echo": "^5.1.1",
    "ionic-mocks": "^1.3.0",
    "lodash": "^4.17.20",
    "moment": "2.24.0",
    "ng-bullet": "^1.0.3",
    "protractor": "^7.0.0",
    "rxjs": "~6.5.5",
    "rxjs-compat": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@capacitor/cli": "2.4.2",
    "@dvsa/mes-search-schema": "1.1.2",
    "@dvsa/mes-test-schema": "3.31.0",
    "@ionic-native-mocks/app-version": "^2.0.12",
    "@ionic-native-mocks/device": "^2.0.12",
    "@ionic/angular-toolkit": "^2.3.0",
    "@ionic/app-scripts": "3.2.4",
    "@ngrx/schematics": "^10.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.9",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "codelyzer": "^6.0.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.2.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-rxjs": "^2.1.5",
    "husky": "^4.3.6",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.7",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "ios"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "run-s test gitSecrets scanRepo"
    }
  }
}
