    "@capacitor/core": "^4.0.0",
    "@capacitor/ios": "^4.0.0",
    "@capacitor/splash-screen": "^4.1.0",
    "@capacitor/status-bar": "^4.0.0",
    "@dvsa/mes-config-schema": "^1.2.2",
    "@dvsa/mes-journal-schema": "1.2.2",
    "@dvsa/mes-search-schema": "1.1.5",
    "@dvsa/mes-test-schema": "3.39.6",
    "@ionic-enterprise/auth": "3.9.0",
