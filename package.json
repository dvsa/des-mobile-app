    "@ionic/core": "^6.3.2",
    "@ng-bootstrap/ng-bootstrap": "^13.1.0",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@sentry/angular": "7.13.0",
    "@sentry/capacitor": "0.10.1",
    "@sentry/tracing": "7.13.0",
    "angular2-signaturepad": "^3.0.4",
    "assert": "^2.0.0",
    "bootstrap": "^5.2.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "capacitor-native-biometric": "^4.1.3",
    "cordova-ios": "6.2.0",
    "cordova-ios-guided-access": "git+https://github.com/wizpanda/cordova-ios-guided-access.git",
    "cordova-plugin-app-version": "^0.1.14",
    "cordova-plugin-emm-app-config": "^1.0.2",
    "cordova-plugin-google-analytics": "^1.9.2",
    "cordova-plugin-insomnia": "^4.3.0",
    "cordova-plugin-ios-disableshaketoedit": "^1.0.0",
    "cordova-plugin-is-debug": "^1.0.0",
    "cordova-plugin-network-information": "^3.0.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
